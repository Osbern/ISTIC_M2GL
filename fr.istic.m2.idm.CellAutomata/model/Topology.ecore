<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Topology"
    nsPrefix="">
  <eClassifiers xsi:type="ecore:EClass" name="Dimension">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="size" eType="#//Integer"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Geometry">
    <eStructuralFeatures xsi:type="ecore:EReference" name="dimensions" lowerBound="1"
        upperBound="-1" eType="#//Dimension"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="neighbors" upperBound="-1"
        eType="#//Neighbor"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nDim" eType="#//Integer"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isCircular" eType="#//Bool"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Neighbor">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="coord" upperBound="-1"
        eType="#//Integer"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="Integer" instanceClassName="java.lang.Integer"/>
  <eClassifiers xsi:type="ecore:EDataType" name="Bool" instanceClassName="java.lang.Boolean"/>
</ecore:EPackage>
